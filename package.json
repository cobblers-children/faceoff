{
  "name": "faceoff",
  "version": "1.3.1",
  "description": "Compare performance across multiple versions of your code",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "ci": "npm run coverage && npm run smoke",
    "unit": "node --allow-natives-syntax --test --test-reporter spec test/unit/*.js",
    "integration": "node --allow-natives-syntax --test --test-reporter spec test/integration/*.js",
    "test": "npm run unit && npm run integration",
    "coverage": "c8 npm run test",
    "watch": "node --allow-natives-syntax --test --watch --test-reporter spec 'test/unit/**'",
    "smoke": "node --allow-natives-syntax examples/prom-client.js && node --allow-natives-syntax examples/p-limit.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cobblers-children/faceoff.git"
  },
  "keywords": [
    "benchmark",
    "bench-node",
    "performance",
    "regression",
    "speed",
    "test"
  ],
  "author": "Jason Marshall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cobblers-children/faceoff/issues"
  },
  "homepage": "https://github.com/cobblers-children/faceoff#readme",
  "engines": {
    "node": "^20 || ^22 || >=24"
  },
  "files": [
    "lib/**"
  ],
  "devDependencies": {
    "c8": "^10.1.3",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.1",
    "chai-spies": "^1.1.0",
    "chai-string": "^1.6.0"
  },
  "c8": {
    "all": true,
    "src": [
      "./lib"
    ],
    "extension": [
      ".js"
    ],
    "report-dir": "./coverage",
    "reporter": "lcov"
  },
  "dependencies": {
    "bench-node": "^0.14.0"
  }
}
